{
  "name": "rgac",
  "version": "1.1.3",
  "description": "Riot Games API Client, written in TypeScript",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "test": "jest",
    "t:c": "npm run test:coverage",
    "test:coverage": "jest --coverage",
    "docs": "rimraf docs && typedoc",
    "prepublish": "npm run build # && npm test",
    "generate-schemas": "ts-node src/tools/generateJoiFromInterfaces.ts",
    "generate-files": "ts-node src/tools/generateEnumsFromFiles.ts",
    "prettier": "prettier --loglevel error --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "from-scratch": "npm run generate-schemas && npm run generate-files && npm run prettier && npm run build && npm run lint && npm run docs && npm run --silent test:coverage",
    "lint": "eslint 'src/**/*.ts' "
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "keywords": [
    "league of legends",
    "riot games",
    "tft",
    "teamfight tactics"
  ],
  "author": "Micha≈Ç Wadas <michalwadas@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/hapi__joi": "^16.0.3",
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.144",
    "@types/node": "^10.17.3",
    "@types/slug": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "mkdirp-promise": "^5.0.1",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "types": "./build/index.d.ts",
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "@types/mkdirp-promise": "^5.0.0",
    "axios": "^0.19.2",
    "axios-rate-limit": "^1.3.0",
    "lodash": "^4.17.20",
    "slug": "^1.1.0",
    "tslib": "^1.13.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run docs && git add docs",
      "pre-push": "npm run build && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ginden/rgac.git"
  }
}
